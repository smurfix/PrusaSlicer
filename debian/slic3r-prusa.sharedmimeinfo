    <comment>Geomview Object File Format</comment>
    <glob pattern="*.off"/>
    <magic priority="50">
      <match value="OFF" type="string" offset="0"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-amf">
    <comment>Additive Manufacturing File Format</comment>
    <glob pattern="*.amf"/>
  </mime-type>
