  <mime-type type="application/sla">
    <comment>Stereolithography File Format</comment>
    <glob pattern="*.stl"/>
  </mime-type>
  <mime-type type="model/x-wavefront-obj">
    <comment>Wavefront Object File</comment>
    <glob pattern="*.obj"/>
  </mime-type>
